    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>95352970-c0f6-4223-ad09-31831137d3ac</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.4" />
